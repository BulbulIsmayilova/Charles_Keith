$smcT5.CustomScripts.TagCode = function(){
    (function BasketScanner(){
        var getATC = setInterval(function(){        
            var img = document.querySelector('.product-line-item .product-line-item-details .item-image img,.pdp-images .js-carousel-inner .carousel-item img');
            var title = document.querySelector('.product-line-item .line-item-name a span,.minibag_modal-product_name');
            var price = document.querySelector('.product-line-item .price-wrap > span:last-of-type,.pdp-prices .prices .price-wrap span:last-of-type');
            if (img && title && price){
                var obj = {};
                obj.img = img.src;
                obj.title = title.textContent;
                obj.price = price.innerText.trim();
                var itemEncoded = btoa(encodeURIComponent(JSON.stringify(obj)));
                $smcT5.CookieManager.create('smc_dyn_item', itemEncoded, 0);
            }
        }, 400);
    })();

    (function cartButtonListener(){
        setInterval(function(){
            $smcT5.CustomScripts.scanViewedItem();
        }, 2000);
    })();
    
    (function UserTimeOnSite(){
        var time = new Date().getTime();
        var startTimeCookie = $smcT5.CookieManager.read('smc_start_time');
        if(!startTimeCookie) {
            $smcT5.CookieManager.create('smc_start_time', time, 0);
        }
        var timeOnsite = Math.floor((time - Number(startTimeCookie)) / 60000);
        $smcT5.CustomScripts.UserTimeOnSite = timeOnsite > 10000000 ? 0 : timeOnsite;
    })();
    
    function deleteBasketBackupData(bbCampaignID){
        var tagData = JSON.parse(atob($smctData.o));
        var uid = $smcT5.tagHelpers.get('uid');
        var did = $smcT5.tagHelpers.get('dv').id;
    
        $.post('https://ep.smct.co/bb-ep/?handle=store', {
            bb_tid: tagData.id,
            bb_uid: uid,
            bb_cid: bbCampaignID || tagData.plugins.basketBackup.id,
            bb_did: did,
            bb_data: "{}",
            delete_data: 1
        });
    }
    
    (function visitedSalePage() {
        var url = window.location.href;
        if (url.indexOf('/sale') > -1) {
            $smcT5.CookieManager.create('smc_visited_sale_page', true, 0);
        }
    })();
    
     // 04-11-2020 - K.S - If there is any sale item in the basket
   (function FindSaleItem(){
        var button = document.querySelector('.adding_to_cart-btn');
        if (button){
            button.addEventListener('click', function(){
                var discountElem = document.querySelector('.pdp-prices .price .tile_discount');
                if(discountElem){
                    $smcT5.CookieManager.create('smc_sale_item_exist', true, 0);     
                }
            })
        }
    })();
    

    
    // Custom basket backup script 
    (function(){
        var isDebug = typeof window.$smcDebugger === 'undefined' ? 0 : 1;
        //Tag Functions
        var TAG = $smcT5;
        var HELPERS = TAG.tagHelpers;
        var DBC = HELPERS.DBC; //isDebug console log
        var DBW = HELPERS.DBW; //isDebug console warn
        
        var $SOURCES = window.$smctResources;
        var $tag_data = {}
        try {
            $tag_data = JSON.parse(atob(window.$smctData.o)); //Attempt base64 version first
        } catch(e) {
            $tag_data = window.$smctData.o;
        }
        
        //User Modulas based on the UserID
        //var userModulas = HELPERS.get('uid') % 100; NOT USED
        
        //Load jQuery - Looks to see if it already exists first.
        var loadJquery = function ( campaign_id, successCallback ) {
            if ( typeof $smcJQ !== "undefined" && $tag_data.config.jQueryForceLoad == 0) {
                DBC('BB - 1.0: $smcJQ INTERNAL, cid:'+campaign_id+'')
                successCallback();
            } else if ( typeof jQuery !== "undefined" && $tag_data.config.jQueryForceLoad == 0) {
                DBC('BB - 1.1: jQuery INTERNAL, cid:'+campaign_id+'');
                window.$smcJQ = jQuery;
                successCallback();
            } else {
                DBC('BB - 1.2: $smcJQ EXTERNAL, cid:'+campaign_id+'');
                var u = $SOURCES.getURL('smc_jq');
                var h = document.getElementsByTagName("head")[0];
                var j = document.createElement('script');
                j.type = 'text/javascript';
                j.src = u;
                j.onload = function () {
                    $smcJQ.noConflict();
                    successCallback();
                };
                h.appendChild(j);
            }
        };
        
        //Load any script function with calllback
        var anyScriptLoader = function (scriptName, successCallback) {
            if( !scriptName || scriptName.trim() === '' ){
                DBW('No Script set for anyScriptLoader() Custom Code');
                return false;
            }
            var h = document.getElementsByTagName("head")[0];
            var j = document.createElement('script');
            j.type = 'text/javascript';
            j.src = scriptName;
            j.charset = "utf-8";
            j.onload = function () {
                successCallback(true);
            };
            h.appendChild(j);
        };
        
        //Load basket backup script
        var loadBasketBackup = function ( campaign_id, successCallback ) {
            var basketBackupScriptName = $SOURCES.getURL('basket_backup_script');
            if(basketBackupScriptName && basketBackupScriptName.trim() !== ''){
                anyScriptLoader(basketBackupScriptName, function () {
                    DBC('BB - 2.0: loadBasketBackup(), cid:'+campaign_id+'');
                    successCallback( );
                });
            }
        };
        
        var bbRunner = function( campaign_id ){
            $smcT5.outputs.basketBackup = {
                'endpoint' : $SOURCES.getURL('basket_backup_endpoint'),
                'campaign_id' : campaign_id,
            };
            loadJquery( campaign_id, function() {
                loadBasketBackup( campaign_id, function() {
                    DBC('BB - 3.0: bbRunner(), cid:'+campaign_id+'');
                });
            });
        }


        var basketBBObject = {
           "CA":410,
           "US":206
        }
        
        var langCookie =  $smcT5.CookieManager.read("smc_select_lang");
        
        //Check that user visits from English site
        if(langCookie === "CA" && basketBBObject.hasOwnProperty('CA') && langCookie !== null){
            DBW('Currently selected Basket Backup campaign: ');
            deleteBasketBackupData(206); //Delete US campaign
            bbRunner(basketBBObject[langCookie]); // Run the BB Canada campaign
        }else if(langCookie === "US" && basketBBObject.hasOwnProperty('US') && langCookie !== null){
            DBW('Currently selected Basket Backup campaign: ');
            deleteBasketBackupData(410); //Delete Canada campaign
            bbRunner(basketBBObject[langCookie]); // Run the BB US campaign
        }
        
    })();
    
    (function cartButtonListener(){
        setInterval($smcT5.CustomScripts.suggestedProductScan, 1500);
    })();
    
    (function initializeLastPurchaed(){
        const url = window.location.href
        const timeStampLS = localStorage.getItem('smc_purchased_timestamp')
        const userPurchased = $smcT5.CookieManager.read('smc_user_purchased_in_session')
        if(!timeStampLS && !userPurchased){
            localStorage.setItem('smc_purchased_timestamp',Date.now())
        }
        if(url.includes('/success') || url.includes('/thank-you')){
            $smcT5.CookieManager.create('smc_user_purchased_in_session', true)
            localStorage.removeItem('smc_purchased_timestamp')
        }
    })();
    
    
      (function viewedCount(){
              if (window.location.href.includes('best-sellers') || document.referrer.includes('best-sellers')) { 
            const cookie = $smcT5.CookieManager.read('smc_bestseller_views')
            if (!cookie) {
                $smcT5.CookieManager.create('smc_bestseller_views',0);
            } else {
                $smcT5.CookieManager.create('smc_bestseller_views', Number(cookie) + 1);
            }
        }
    })();
      // J.D Checks if items in the collection has been added to cart - Has to be in a set interval because the page loads products async
    (function springSummerInCart(){
        const urls = ['/spring-summer','/au/curated/new-season'];
        const exist = urls.some((el) => window.location.href.includes(el) || document.referrer.includes(el));
        if(exist){
            $smcT5.CookieManager.create('smc_summer_viewed', true);
            const search = setInterval(() => {
                const button = document.querySelectorAll('.js-pdp-add-to-cart, .js-tile-add-to-cart-btn:not(.product_set .js-tile-add-to-cart-btn, .recommendations-content .js-tile-add-to-cart-btn)');
                if (button.length > 0){
                    button.forEach((el) =>{
                        if(!el.hasAttribute('smc-summer-listener')){
                            el.setAttribute('smc-summer-listener', true);
                            el.addEventListener('click', function(){
                                $smcT5.CookieManager.create('smc_summer_in_cart', true);
                            })
                        }
                    })
                }
            }, 5000)
        }
    })();
    
    
};

$smcT5.CustomScripts.suggestedProductScan = function () {
    var itemContainer = document.querySelectorAll('.product-detail + .recommendations-like .recommendations-content .swiper-wrapper .product-tile');
    if (itemContainer.length > 0) {
        var productDetails = [];
        for(var i = 0; i < itemContainer.length; i++){
            var product = itemContainer[i];
            if(i < 4) {
                var imageEl = product.querySelector('.product_tile-image.active a img');
                var titleEl = product.querySelector('.product_tile-name .product_tile-link');
                var priceEl = product.querySelector('.tile-body > .price .price-wrap span.sales');
                var linkEl = product.querySelector('.product_tile-image.active a');
                if(imageEl && titleEl && priceEl && linkEl){
                    var image = imageEl.src;
                    var title = titleEl.textContent.trim();
                    var price = priceEl.textContent.trim();
                    var link = linkEl.href;
                    productDetails.push({ image: image, name: title, price: price, link: link });
                }
            }
        }
        var itemEncoded = btoa(encodeURIComponent(JSON.stringify(productDetails)));
        $smcT5.CookieManager.create('smc_sugg_porduct', itemEncoded, 0);
    }        
};

$smcT5.CustomScripts.BasketValue = function () {
    var cookie = $smcT5.CookieManager.read('CART_AMOUNT');
    if(cookie){
        return Number(cookie);
    }else{
        return 0;
    }
};

$smcT5.CustomScripts.OosAvailable = function () {
    var cookie = $smcT5.CookieManager.read('smc_sugg_porduct');
    var oos = document.querySelector('.product-detail .badge.pdp-availability_badge');
    return cookie && oos && oos.textContent.trim() === 'Out of Stock' ? true : false;
};

$smcT5.CustomScripts.BasketCount = function () {
    var basketCount = 0;
    var localStorageData = localStorage['paid-products'] ? JSON.parse(localStorage['paid-products']).data : [];
    
    if(localStorageData) {
        
        for(var i = 0; i < localStorageData.length; i++) {
            var product = localStorageData[i];
            
            if(product.quantity) basketCount += product.quantity;
            else basketCount += 1;
        }
    }
    
    return basketCount;
}

$smcT5.CustomScripts.LoggedIn = function () {
	return !!document.querySelector('.header_login-name');
};

//J.M - 20/11/2020 - checks if user is on a sale page
$smcT5.CustomScripts.NotOnSalePage = function(){
    var url = window.location.href;
    if (
        url.indexOf('/sale') > -1
    ){
        return false;
    } else {
        var breadcrumbs = document.querySelectorAll('.breadcrumb-item span');
        if (breadcrumbs.length > 0){
            for (i = 0; i < breadcrumbs.length; i++){
                var crumb = breadcrumbs[i].textContent;
                if (crumb.indexOf('Sale') > -1){
                    return false;
                }
            }
        }
    }
    return true;
}

$smcT5.CustomScripts.CurrentCountry = function(){
    var currentCurrency = document.querySelector('.country_switch-current_country');
    if(currentCurrency){
        var country = currentCurrency.innerText.trim().replace(',','');
        $smcT5.CookieManager.create("smc_select_lang", country, 60*24*30);
        if(country === "United States Of America (EN)"){
            $smcT5.CookieManager.create("smc_select_lang", "US", 60*24*30);
            return "US";
        }else if(country === "Canada"){
            $smcT5.CookieManager.create("smc_select_lang", "CA", 60*24*30);
            return "CA";
        }
        else if(country === "Australia"){
             $smcT5.CookieManager.create("smc_select_lang", "AU", 60*24*30);
                return "AU";
        }
        else if(country === "United States Of America (ES)"){
             $smcT5.CookieManager.create("smc_select_lang", "ES", 60*24*30);
                return "ES";
        }else{
            return country;
        }
    }
};

$smcT5.CustomScripts.ShowSiteEntry = function(){
    var cookie = $smcT5.CookieManager.read('smc_site_entry_opened');
    return cookie ? false : true;
}

$smcT5.CustomScripts.ShowClickFrenzy = function(){
    var cookie = $smcT5.CookieManager.read('smc_site_entry_opened');
    var cookie2 = $smcT5.CookieManager.read('smc_click_frenzy_opened');
    var pv = $smcT5.tagHelpers.get('spv');
    if (pv === 2 && cookie && !cookie2){
        return true;
    } else if (pv > 2 && !cookie2){
        $smcT5.CookieManager.create('smc_click_frenzy_opened', true, 60*24*30)
        return false;
    } else {
        return false;
    }
}

$smcT5.CustomScripts.ShowLeadOl = function(){
    var cookie = $smcT5.CookieManager.read('smc_lead_opened');
    var cookie2 = $smcT5.CookieManager.read('smc_site_entry_opened');
    var cookie3 = $smcT5.CookieManager.read('smc_click_frenzy_opened');
    return !cookie && cookie2 && cookie3 ? true : false;
}

$smcT5.CustomScripts.MlItemPrice = function(){
    var salesPrice = document.querySelector('.price .sales-discount');
    if (salesPrice){
        var price = document.querySelector('.price .strike-through');
        return price ? $smcT5.tagHelpers.priceFinder.convertForMaths(price.textContent.trim()) : null;
    } else {
        var price = document.querySelector('.price');
        return price ? $smcT5.tagHelpers.priceFinder.convertForMaths(price.textContent.trim()) : null;
    }
}

$smcT5.CustomScripts.MlItemSalePrice = function(){
    var salesPrice = document.querySelector('.price .sales-discount');
    return salesPrice ? $smcT5.tagHelpers.priceFinder.convertForMaths(salesPrice.textContent.trim()) : null;
}

$smcT5.CustomScripts.Currency = function(){
    return 'USD';
}



$smcT5.CustomScripts.OrderValue = function(){
    var el = document.querySelector('.summary_subtotal-value .grand-total-sum');
    return el ? $smcT5.tagHelpers.priceFinder.convertForMaths(el.textContent) : 0;
}

$smcT5.CustomScripts.scanViewedItem = function(){   
    
    // Get the viewed item cookie if it is already exist
    const cookie = $smcT5.CookieManager.read('smc_viewed_items');
    
    // Get product selectors from product page
    const imageEl = document.querySelector('.product_gallery .carousel-item img,.swiper-slide.swiper-slide-active picture img');
    const titleEl = document.querySelector('.js-product-name');
    const priceEl = document.querySelector('.price .price-wrap .sales');
    const skuEl = document.querySelector('.js-pdp-add-to-cart');
    const link = window.location.href;

    if(!imageEl || !titleEl || !priceEl || !skuEl) return;

    const image = imageEl.src;
    const title = titleEl.innerText.trim();
    const price = priceEl.textContent.trim();
    const sku = skuEl.getAttribute('data-pid');
    
    // Check if there is any item was viewed before
    if(cookie){
        const item = JSON.parse(decodeURIComponent(atob(cookie)));
        
        
        // Avoid pushing the item if it is same product        
        const findItem = item.map((el) => el.sku === sku).every((bool) => bool === false);
        if(!findItem) return;
        
        // Add product to the array if already not exists        
        item.push({ image: image, name: title, price: price, sku: sku, link: link });       
        
        const itemEncoded = item.length >= 3 ? btoa(encodeURIComponent(JSON.stringify(item.slice(Math.max(item.length - 3, 0))))) : btoa(encodeURIComponent(JSON.stringify(item)));
        $smcT5.CookieManager.create('smc_viewed_items', itemEncoded, 0);
        return;
    }
    
    // Product array
    const productArr = [];
    
    // Push item to the array if user is on product page but have not viewed any item before
    if(!cookie){ 
        productArr.push({ image: image, name: title, price: price, sku: sku, link: link });
       
        const itemEncoded = btoa(encodeURIComponent(JSON.stringify(productArr)));
        $smcT5.CookieManager.create('smc_viewed_items', itemEncoded, 0);
    }
};


$smcT5.CustomScripts.RecominderItemExist = function(){
    const cookie = $smcT5.CookieManager.read('smc_viewed_items');
    return cookie && JSON.parse(decodeURIComponent(atob(cookie))).length >= 3 ? true : false;
};


$smcT5.CustomScripts.DynamicItemExist = function(){
    var cookie = $smcT5.CookieManager.read('smc_dyn_item');
    return cookie ? true : false;
};

$smcT5.CustomScripts.PageViews = function(){
    const cookie = $smcT5.CookieManager.read('smc_spv')
    return cookie ? cookie : null
}

// K.S - Add item to cart
$smcT5.CustomScripts.AddItem = function(sku, country, lang) {
    const xhr = new XMLHttpRequest();
    xhr.open('POST', `https://www.charleskeith.com/on/demandware.store/Sites-ck-${country}-Site/${lang}/Cart-AddProduct`, true);
    xhr.setRequestHeader('Content-type', 'application/x-www-form-urlencoded');
    xhr.onload = () => {
        $smcT5.tagHelpers.DBC.custom('SUCCESS', '#673AB7')('ITEM ADDED');
    };
    xhr.send(`pid=${sku}&quantity=1&personalizedInitials=&options=%5B%5D`);
}

$smcT5.CustomScripts.OnHomePage = function(){
    const url = window.location.href
    if(url === 'https://www.charleskeith.com/au' || url === 'https://www.charleskeith.com/ca' || url === 'https://www.charleskeith.com/us' || url === 'https://www.charleskeith.com/es'){
        return true
    }
    return false
};

// DaysVisited
// This is a mildly expensive function,
// so keeping it out of the magic interval
(() => {
    const key = 'smc_DaysVisited';
    const prevDates = readLs(key, []);
    const today = new Date();
    
    const breakDownDate = (date) => {
        const day = date.getDay() + 1;
        const month = date.getMonth();
        const year = date.getYear();
        
        return [ day, month, year ];
    };
    const [ currentDay, currentMonth, currentYear ] = breakDownDate(today);
    
    const duplicates = prevDates.filter((date) => {
        const [ day, month, year ] = breakDownDate(new Date(date));
        return (
            day === currentDay &&
            month === currentMonth &&
            year === currentYear
        );
    });
    const isUniqueDay = !duplicates.length;
    
    if(isUniqueDay) writeLs(key, [...prevDates, today.getTime()]);
    
    $smcT5.CustomScripts.DaysVisited = function() {
        return readLs(key, []).length;
    }
})();


// DaysVisited
// This is a mildly expensive function,
// so keeping it out of the magic interval
(() => {
    const key = 'smc_DaysVisited';
    const prevDates = readLs(key, []);
    const today = new Date();
    
    const breakDownDate = (date) => {
        const day = date.getDay() + 1;
        const month = date.getMonth();
        const year = date.getYear();
        
        return [ day, month, year ];
    };
    const [ currentDay, currentMonth, currentYear ] = breakDownDate(today);
    
    const duplicates = prevDates.filter((date) => {
        const [ day, month, year ] = breakDownDate(new Date(date));
        return (
            day === currentDay &&
            month === currentMonth &&
            year === currentYear
        );
    });
    const isUniqueDay = !duplicates.length;
    
    if(isUniqueDay) writeLs(key, [...prevDates, today.getTime()]);
    
    $smcT5.CustomScripts.DaysVisited = function() {
        return readLs(key, []).length;
    }
})();

// HasPurchased
// Need to avoid setting a click listener on an interval
(() => {
    const key = 'smc_HasPurchased';
    const selector = '.js-place-order, .js-apple-pay-button, ' +
        '.js-google-pay-button-container, .js-paypal-btn-wrapper, ' +
        '.js-google-pay-express-button-container';
    const buyButtons = document.querySelectorAll(selector);
 
    buyButtons.forEach((button) => {
        button.addEventListener('click', () => {
            writeLs(key, true);
        });
    });

    $smcT5.CustomScripts.HasPurchased = function() {
        return readLs(key, false);
    }
})();

// SaleInCart
// Need to avoid setting a click listener on an interval
(() => {
    const key = 'smc_SaleInCart';
    const buyButtons = document.querySelectorAll('.js-add-to-cart');
    
    buyButtons.forEach((button) => {
        button.addEventListener('click', () => {
            writeLs(key, true);
        });
    });
    
    $smcT5.CustomScripts.SaleInCart = function() {
        const basketSelector = '.product-summary';
        const checkoutSelector = '.js-cart-page';
        const saleSelector = '.discount_percentage';
        const basketSaleSelector = `${basketSelector} ${saleSelector}, ` +
            `${checkoutSelector} ${saleSelector}`;
        
        const basketEl = document.querySelector(basketSelector);
        const checkoutEl = document.querySelector(checkoutSelector);
        const saleEl = document.querySelector(basketSaleSelector);
        
        // If basket available, read it. Otherwise check if they clicked
        // add to cart on sale item
        const basketAvailable = Boolean(basketEl || checkoutEl);
        
        if(basketAvailable && !saleEl) {
            writeLs(key, false);
            return false;
        }
        
        if(basketAvailable && saleEl) return true;
        
        if(!basketAvailable) return readLs(key, false);
    }
})();

$smcT5.CustomScripts.DaysWithoutPurchased = function(){
    const timeStampLS = localStorage.getItem('smc_purchased_timestamp');
    if(!timeStampLS) return 0;
    const now = Date.now();
    const timeStamp = Number(timeStampLS);
    const difference = now - timeStamp;
    return Math.floor(difference / (1000 * 60 * 60 * 24))
}

$smcT5.CustomScripts.BestSellerViewed = function(){
    const url = window.location.href;
    const ref = document.referrer;
    const viewed = localStorage.getItem('smc_best_seller_viewed');
    const userPurchased = $smcT5.CookieManager.read('smc_user_purchased_in_session_two');
    let count = 0;

    if(url.includes('best-sellers') || ref.includes('best-sellers')){
        count = count + 1
        if(count >= 3){
            localStorage.setItem('smc_best_seller_viewed', count);
        }
    }

    if(url.includes('/success') || url.includes('/thank-you')){
        $smcT5.CookieManager.create('smc_user_purchased_in_session_two', true);
        localStorage.removeItem('smc_best_seller_viewed');
    }   

    return viewed ? viewed : null;
}
$smcT5.CustomScripts.SummerCollectionInCart = function(){
    const cookie = $smcT5.CookieManager.read('smc_summer_in_cart')
    return cookie ? true : false
}

$smcT5.CustomScripts.SummerCollectionViewed = function(){
     const cookie = $smcT5.CookieManager.read('smc_summer_viewed')
     return cookie ? true : false
}

$smcT5.CustomScripts.BestSellersViewedCount = function() {
    const cookie = $smcT5.CookieManager.read('smc_bestseller_views');
    return cookie ? Number(cookie) : 0;
}

$smcT5.CustomScripts.NewArrivalsPageVisited = function(){
    var url = window.location.href;
    var cookie = $smcT5.CookieManager.read('smc_new_arrivals_visited');// The value in the '' needs to match with value in the create one
    if (url.indexOf('/new-arrivals') > -1 && !cookie){ //whatever you put in the '' is what it will look for in the url, same as what you do with url white/blacklists
        $smcT5.CookieManager.create('smc_new_arrivals_visited', true); //This needs to match with the read one
    }
    return cookie ? true : false;
}

$smcT5.CustomScripts.MemorialPageVisited = function(){
